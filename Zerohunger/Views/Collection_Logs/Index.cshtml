@model IEnumerable<Zerohunger.EF.Collection_Logs>

@{
    ViewBag.Title = "Index";
}

<h2 class="text-center text-primary">Employee</h2>

<ul class="nav nav-tabs bg-success justify-content-center">

    <li class="nav-item">
        <a class="nav-link active" aria-current="page" href="/Restaurants/Index">Restaurant List</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" href="/Collect_Requests/Home">Collection Request</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" href="/Collection_Logs/Index">Collection Logs</a>
    </li>

    <li>
        <a href="~/Home/AddUser">Log Out</a>
    </li>
</ul>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RequestId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Collection_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distribution_date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RequestId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Collection_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distribution_date)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
